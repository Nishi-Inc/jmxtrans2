{
    "outputWriters": [
        {
            "@class": "org.jmxtrans.output.writers.ConsoleOutputWriter"
        },
        {
            "@class": "org.jmxtrans.output.writers.GraphitePlainTextTcpOutputWriter",
            "settings": {
                "host": "${graphite.host:localhost}",
                "port": "${graphite.port:2003}",
                "namePrefix": "servers.${server.name:localhost}."
            }
        }
    ],
    "queryIntervalInSeconds": 5,
    "numQueryThreads": 3,
    "exportIntervalInSeconds": 10,
    "numExportThreads": 2,
    "exportBatchSize": 30
}
